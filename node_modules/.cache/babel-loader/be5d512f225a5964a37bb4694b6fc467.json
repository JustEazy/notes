"\r\n                    :loading=\"submitting\"\r\n                    label=\"Зарегистрироваться\"\r\n                    class=\"q-mt-md\"\r\n                    color=\"black\"\r\n            >\r\n                <template v-slot:loading>\r\n                    <q-spinner-facebook />\r\n                </template>\r\n            </q-btn>\r\n        </q-form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Register\",\r\n        data() {\r\n            return {\r\n                form: {\r\n                    name: \"\",\r\n                    email: \"\",\r\n                    password: \"\"\r\n                },\r\n                error: null,\r\n                isPwd: true,\r\n                submitting: false\r\n            };\r\n        },\r\n        methods: {\r\n            submit() {\r\n                this.submitting = true\r\n                firebase\r\n                    .auth()\r\n                    .createUserWithEmailAndPassword(this.form.email, this.form.password)\r\n                    .then(data => {\r\n                        data.user\r\n                            .updateProfile({\r\n                                displayName: this.form.name\r\n                            })\r\n                            .then(() => {\r\n                            });\r\n                    })\r\n                    .catch(err => {\r\n                        this.error = err.message;\r\n                    });\r\n                setTimeout(() => {\r\n                    // delay simulated, we are done,\r\n                    // now restoring submit to its initial state\r\n                    this.submitting = false\r\n                }, 3000)\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .form-input {\r\n        width: 70%;\r\n    }\r\n\r\n    .q-form {\r\n        margin-top: 10%;\r\n        margin-left: 20%;\r\n    }\r\n    .q-btn{\r\n        width: 70%;\r\n        margin-right: 150px;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/auth"}]}